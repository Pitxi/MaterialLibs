{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-material-components-showcase": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/ngx-material-components-showcase",
      "sourceRoot": "projects/ngx-material-components-showcase/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ngx-material-components-showcase",
            "index": "projects/ngx-material-components-showcase/src/index.html",
            "main": "projects/ngx-material-components-showcase/src/main.ts",
            "polyfills": "projects/ngx-material-components-showcase/src/polyfills.ts",
            "tsConfig": "projects/ngx-material-components-showcase/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ngx-material-components-showcase/src/favicon.ico",
              "projects/ngx-material-components-showcase/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "projects/ngx-material-components-showcase/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/ngx-material-components-showcase/src/environments/environment.ts",
                  "with": "projects/ngx-material-components-showcase/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "NgxMaterialComponentsShowcase:build:production"
            },
            "development": {
              "browserTarget": "NgxMaterialComponentsShowcase:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "NgxMaterialComponentsShowcase:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-material-components-showcase/src/test.ts",
            "polyfills": "projects/ngx-material-components-showcase/src/polyfills.ts",
            "tsConfig": "projects/ngx-material-components-showcase/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-material-components-showcase/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ngx-material-components-showcase/src/favicon.ico",
              "projects/ngx-material-components-showcase/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "projects/ngx-material-components-showcase/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "@pitxi/ngx-material-components": {
      "projectType": "library",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/pitxi/ngx-material-components",
      "sourceRoot": "projects/pitxi/ngx-material-components/src",
      "prefix": "ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/pitxi/ngx-material-components/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/pitxi/ngx-material-components/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/pitxi/ngx-material-components/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/pitxi/ngx-material-components/src/test.ts",
            "tsConfig": "projects/pitxi/ngx-material-components/tsconfig.spec.json",
            "karmaConfig": "projects/pitxi/ngx-material-components/karma.conf.js"
          }
        }
      }
    },
    "@pitxi/ngx-mat-data-filter": {
      "projectType": "library",
      "root": "projects/pitxi/ngx-mat-data-filter",
      "sourceRoot": "projects/pitxi/ngx-mat-data-filter/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/pitxi/ngx-mat-data-filter/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/pitxi/ngx-mat-data-filter/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/pitxi/ngx-mat-data-filter/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/pitxi/ngx-mat-data-filter/src/test.ts",
            "tsConfig": "projects/pitxi/ngx-mat-data-filter/tsconfig.spec.json",
            "karmaConfig": "projects/pitxi/ngx-mat-data-filter/karma.conf.js"
          }
        }
      }
    }
  }
}
